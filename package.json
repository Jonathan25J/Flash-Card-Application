{
  "name": "flash-cards-application",
  "version": "1.3.6",
  "author": "Jonaqhan",
  "description": "Flash Cards Application that can be used for learning",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "trace": "electron --trace-warnings .",
    "server": "nodemon --watch . --ignore resources/ --exec \"npm run build && node src/server/server\"",
    "clean": "rimraf resources/website",
    "build": "npm run clean && rollup -c",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@web/rollup-plugin-html": "^2.1.2",
    "electron": "^28.2.3",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "rollup": "^4.12.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-watch": "^3.2.2"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^3.0.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@web/rollup-plugin-copy": "^0.5.1",
    "chokidar": "^3.6.0",
    "ejs": "^3.1.9",
    "electron-store": "^8.2.0",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "fs-extra": "^11.2.0",
    "lit": "^3.1.2",
    "postcss": "^8.4.35",
    "postcss-url": "^10.1.3",
    "proper-lockfile": "^4.1.2",
    "rimraf": "^5.0.5",
    "rollup-plugin-import-css": "^3.4.0",
    "uuid": "^9.0.1"
  }
}
